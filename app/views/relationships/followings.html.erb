<h1>Follow Users</h1>
  <% if @users.present? %>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th colspan="3"></th>
          <th>name</th>
          <th colspan="3"></th>
          <th></th>
          <th colspan="3"></th>
          <th></th>
          <th colspan="3"></th>
        </tr>
      </thead>
    <% @users.each do |user| %>
      <tbody>
        <tr>
          <td>
          <%= image_tag user.get_profile_image(100,100) %>
          </td>
          <td colspan="3"></td>
          <td><%= user.name %></td>
          <td colspan="3"></td>
          <td>フォロー<%= user.followings.count %></td>
          <td colspan="3"></td>
          <td>フォロワー<%= user.followers.count %></td>
          <td colspan="3"></td>
          <td><%= link_to 'Show', user_path(user) %></td>
        </tr>
      </tbody>
    <% end %>
  <% else %>
    <p>ユーザーはいません</p>
  <% end %>
</table>